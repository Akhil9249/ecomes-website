
<%- include('../partials/admin-header') %>


        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">
            <div class="page-header" style="display: flex;">
     
              <div style="flex: 5;"></div>
              <div style="flex: 3;"><h3 class="page-title">Coupon Edit</h3></div>
              
              <div style="flex: 3;"></div>

            </div>
            <div class="row" style="display: flex;">
              <div style="flex: 1;"></div>
              <div class="col-12 grid-margin stretch-card" style="flex: 2;">
                <div class="card">
                  <div class="card-body">
                    <!-- <h4 class="card-title">Basic form elements</h4> -->
                    <form class="forms-sample"  id="update-coupon" method="post">
                      <div class="form-group">
                        <label for="exampleInputName1">Name</label>
                        <input type="hidden" class="form-control" value="<%= coupon._id%>" name="id" id="exampleInputName1" placeholder="">
                        <input type="text" class="form-control" value="<%= coupon.name%>" name="name" id="exampleInputName1" placeholder="">

                      </div>

                      <div class="form-group">
                        <label for="exampleInputName1">Code</label>
                        <input type="text" class="form-control" value="<%= coupon.code%>" name="code" id="exampleInputName1" placeholder="">
                      </div>

                      <div class="form-group">
                        <label for="exampleInputName1">offer percentage</label>
                        <input type="text" class="form-control" value="<%= coupon.percent%>" name="percentage" id="exampleInputName1" placeholder="">
                      </div>

                      <div class="form-group">
                        <label for="exampleInputName1">Max off</label>
                        <input type="text" class="form-control" value="<%= coupon.maxoff%>" name="maxoff" id="exampleInputName1" placeholder="">
                      </div>

                      <div class="form-group">
                        <label for="exampleInputName1">Expry Date</label>
                        <input type="text" class="form-control" value="<%= coupon.expdt%>" name="expdt" id="exampleInputName1" placeholder="">
                      </div>

                      <div class="form-group">
                        <label for="exampleInputName1">Count</label>
                        <input type="text" class="form-control" value="<%= coupon.count%>" name="count" id="exampleInputName1" placeholder="">
                      </div>
                      
                      
                      <div style="text-align: center;">
                        <button type="submit" class="btn btn-primary mr-2" >Submit</button>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
              <div style="flex: 1;"></div>

            </div>
          </div>
          <!-- content-wrapper ends -->
          <!-- partial:../../partials/_footer.html -->
          
          <%- include('../partials/admin-footer') %>